<prog> ::= <classDecl><progBody>
<classDecl> ::= EPSILON
<progBody> ::= program<funcBody>;<funcDef>
Match success PROGRAM
<funcBody> ::= {<varDeclStatementHead>}
Match success OPENCURLYPAR
<varDeclStatementHead> ::= int id<arraySize>; <varDeclStatementHead>
Match success INT
Match success IDENTIFIER
<arraySize> ::= EPSILON
Match success SEMICOLON
<varDeclStatementHead> ::= int id<arraySize>; <varDeclStatementHead>
Match success INT
Match success IDENTIFIER
<arraySize> ::= EPSILON
Match success SEMICOLON
<varDeclStatementHead> ::= id <varDeclStatementTail>
Match success IDENTIFIER
<varDeclStatementTail> ::= <indice><idnest><assignOp><expr> ; <statement>
<indice> ::= EPSILON
<idnest> ::= EPSILON
<assignOp> ::= =
Match success ASSIGNMENT
<expr> ::= <arithExpr><exprTail>
<arithExpr> ::= <term><arithExprTail>
<term> ::= <factor><termTail>
<factor> ::= id<factorTail>
Match success IDENTIFIER
<factorTail> ::= (<aParams>)
Match success OPENPAR
<aParams> ::= <expr><aParamsTail>
<expr> ::= <arithExpr><exprTail>
<arithExpr> ::= <term><arithExprTail>
<term> ::= <factor><termTail>
<factor> ::= numint
Match success NUMINT
<termTail> ::= EPSILON
<arithExprTail> ::= <addOp><term><arithExprTail>
<addOp> ::= +
Match success ADD
<term> ::= <factor><termTail>
<factor> ::= numint
Match success NUMINT
<termTail> ::= EPSILON
<arithExprTail> ::= EPSILON
<exprTail> ::= EPSILON
<aParamsTail> ::= ,<expr> <aParamsTail>
Match success COMMA
<expr> ::= <arithExpr><exprTail>
<arithExpr> ::= <term><arithExprTail>
<term> ::= <factor><termTail>
<factor> ::= id<factorTail>
Match success IDENTIFIER
<factorTail> ::= EPSILON
<termTail> ::= EPSILON
<arithExprTail> ::= EPSILON
<exprTail> ::= EPSILON
<aParamsTail> ::= ,<expr> <aParamsTail>
Match success COMMA
<expr> ::= <arithExpr><exprTail>
<arithExpr> ::= <term><arithExprTail>
<term> ::= <factor><termTail>
<factor> ::= numint
Match success NUMINT
<termTail> ::= EPSILON
<arithExprTail> ::= EPSILON
<exprTail> ::= EPSILON
<aParamsTail> ::= EPSILON
Match success CLOSEPAR
<termTail> ::= EPSILON
<arithExprTail> ::= EPSILON
<exprTail> ::= EPSILON
Match success SEMICOLON
<statement> ::= EPSILON
Match success CLOSECURLYPAR
Match success SEMICOLON
<funcDef> ::= <funcHead><funcBody>; <funcDef>
<funcHead> ::= <type>id(<fParams>)
<type> ::= int
Match success INT
Match success IDENTIFIER
Match success OPENPAR
<fParams> ::= <type>id<arraySize><fParamsTail>
<type> ::= int
Match success INT
Match success IDENTIFIER
<arraySize> ::= EPSILON
<fParamsTail> ::= ,<type>id<arraySize> <fParamsTail>
Match success COMMA
<type> ::= int
Match success INT
Match success IDENTIFIER
<arraySize> ::= EPSILON
<fParamsTail> ::= ,<type>id<arraySize> <fParamsTail>
Match success COMMA
<type> ::= int
Match success INT
Match success IDENTIFIER
<arraySize> ::= EPSILON
<fParamsTail> ::= EPSILON
Match success CLOSEPAR
<funcBody> ::= {<varDeclStatementHead>}
Match success OPENCURLYPAR
<varDeclStatementHead> ::= return(<expr>); <statement>
Match success RETURN
Match success OPENPAR
<expr> ::= <arithExpr><exprTail>
<arithExpr> ::= <term><arithExprTail>
<term> ::= <factor><termTail>
<factor> ::= id<factorTail>
Match success IDENTIFIER
<factorTail> ::= EPSILON
<termTail> ::= EPSILON
<arithExprTail> ::= <addOp><term><arithExprTail>
<addOp> ::= +
Match success ADD
<term> ::= <factor><termTail>
<factor> ::= id<factorTail>
Match success IDENTIFIER
<factorTail> ::= EPSILON
<termTail> ::= EPSILON
<arithExprTail> ::= <addOp><term><arithExprTail>
<addOp> ::= +
Match success ADD
<term> ::= <factor><termTail>
<factor> ::= id<factorTail>
Match success IDENTIFIER
<factorTail> ::= EPSILON
<termTail> ::= EPSILON
<arithExprTail> ::= EPSILON
<exprTail> ::= EPSILON
Match success CLOSEPAR
Match success SEMICOLON
<statement> ::= EPSILON
Match success CLOSECURLYPAR
Match success SEMICOLON
<funcDef> ::= EPSILON
Match success ENDOFFILE
